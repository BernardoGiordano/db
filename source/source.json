		"scripts": {
			"linux_3ds": [
				{
					"type": "downloadRelease",
					"repo": "xerpi/linux_3ds",
					"file": "linux-3ds-.*\\.zip",
					"output": "/linux-3ds.zip",
					"message": "Downloading linux-3ds zip..."
				},
				{
					"type": "extractFile",
					"file": "/linux-3ds.zip",
					"input": "linux-3ds-.*/firm_linux_loader.firm",
